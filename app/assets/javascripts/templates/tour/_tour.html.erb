<h2>{{walk.title}}</h2>
<div ng-repeat="tour in tours">
  <div class="col-xs-6 col-md-3">
    <a href="#" class="thumbnail">
      <img ng-src={{tour.image}} alt="{{tour.exhibit}}" >
      <div class="caption">
        <h4>{{tour.exhibit}}</h4>
        <p>{{tour.text}}</p>
      </div>
    </a>
  </div>
</div>
