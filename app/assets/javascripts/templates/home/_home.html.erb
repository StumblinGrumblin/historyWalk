<h4>Audio Tours</h4>
<div class="row" ng-repeat="tour in tours">
  <div class="col-xs-10 col-xs-offset-1 col-md-3">
    <a href="tour/{{$index}}" class="thumbnail">
      <div class="caption homeTours">
        <p class="pull-right">
          <span class="badge">{{tour.exhibits.length}}</span> stops
        </p>
        <h4>{{tour.title}}</h4>
      </div>
      <img ng-src={{tour.cover}} >
    </a>
  </div>

</div>
