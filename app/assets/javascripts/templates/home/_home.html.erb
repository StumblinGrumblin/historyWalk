<h4>Audio Tours</h4>
<div class="row" ng-repeat="walk in walks">
  <div class="col-xs-10 col-xs-offset-1 col-md-3">
    <a href="tours/{{$index}}" class="thumbnail">
      <div class="caption homeTours">
        <p class="pull-right">
          <span class="badge">{{walk.walk.length}}</span> stops
        </p>
        <h4>{{walk.title}}</h4>
      </div>
      <img ng-src={{walk.cover}} >
    </a>
  </div>

</div>
